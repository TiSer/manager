jQuery('#dialog').dialog("close");
<% expence =  @milestone.expence %>
<% income =  @milestone.amount %>
$("#m"+<%= @milestone.id %>).text(<%= income %>);
profit_cell = $("#p"+<%= @milestone.id %>);
profit_cell.removeClass("red_bg")

        <% if expence.instance_of? Float %>
           r = '<%= two_numbers_after_point(income.to_f - expence) %>'
           if ( r <= 0 ) {
             profit_cell.addClass("red_bg")
           }
         <% else %>
           r = 'N/A'
         <% end %>
profit_cell.text(r);

